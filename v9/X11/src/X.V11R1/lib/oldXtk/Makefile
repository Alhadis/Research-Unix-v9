OBJS = Actions.o Alloc.o ArgList.o AsciiSink.o Boolean.o ButtonBox.o \
	Command.o Cursors.o Clock.o Debug.o Dialog.o DiskSrc.o \
	Error.o Event.o Form.o GCManager.o Geometry.o Init.o \
	Label.o Load.o Menu.o Message.o NextEvent.o Resources.o \
	Scroll.o ScrollMgr.o ScrollWin.o StringSrc.o Text.o TextActs.o \
	TextKinds.o Util.o VPane.o

.c.o:
	$(CC) -c $(CFLAGS) $*.c

all:: liboldXtk.a

liboldXtk.a: $(OBJS)
	rm -f $@
	ar clq $@ $(OBJS)
	ranlib $@

all:: liboldXtk11.a
liboldXtk11.a: liboldXtk.a
	rm -f $@
	ln -s liboldXtk.a $@

lintlib:: llib-loldXtk.ln

llib-loldXtk.ln: $(SRCS)
	rm -f $@
	$(LINT) -ColdXtk $(DEFINES) ${INCLUDES} $(SRCS)

install:: liboldXtk.a
	$(INSTALL) $(INSTLIBFLAGS) liboldXtk.a $(USRLIBDIR)
	ranlib $(USRLIBDIR)/liboldXtk.a

install::
	rm -f $(USRLIBDIR)/liboldXtk11.a
	ln $(USRLIBDIR)/liboldXtk.a $(USRLIBDIR)/liboldXtk11.a

install:: llib-loldXtk.ln
	$(INSTALL) $(INSTLIBFLAGS) llib-loldXtk.ln $(USRLIBDIR)

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

lint:
	$(LINT) -buz ${INCLUDES} \
		-DLIBDIR=\"${LIBDIR}\" \
		${SRCS} \
		$(XLIBSRC)/llib-lX.ln

clean::
	$(RM_CMD) \#*

Makefile:: Imakefile \
	$(UTILSRC)/imake.includes/Imake.tmpl \
	$(UTILSRC)/imake.includes/Imake.rules \
	$(UTILSRC)/imake.includes/$(MACROFILE)
		-rm -f Makefile.bak; mv Makefile Makefile.bak
		$(IMAKE_CMD) -DTOPDIR=$(TOP)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

