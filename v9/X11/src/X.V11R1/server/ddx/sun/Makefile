.c.o:
	$(CC) -c $(CFLAGS) $*.c

all:: $(OFILES) kbd_mode

all:: libsun.a

libsun.a: $(OBJS)
	rm -f $@
	ar clq $@ $(OBJS)
	ranlib $@

kbd_mode:  kbd_mode.o
	rm -f $@
	$(CC) -o $@  kbd_mode.o  $(LDFLAGS)

relink::
	rm -f kbd_mode
	$(MAKE) $(MFLAGS) kbd_mode

clean::
	rm -f kbd_mode

install:: kbd_mode
	$(INSTALL) -c $(INSTALLFLAGS) kbd_mode $(BINDIR)

install:: kbd_mode.man
	$(INSTALL) -c -m 0664 kbd_mode.man $(MANDIR)/kbd_mode.n

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

install:: Xsun.man
	$(INSTALL) -c -m 0664 Xsun.man $(MANDIR)/Xsun.n

lint: ${CFILES} ${LIBCFILES}
	$(LINT) $(LFLAGS) -Clibsun $(INCLUDES) $(SRCS) >lint.out 2>&1

clean::
	$(RM_CMD) \#*

Makefile:: Imakefile \
	$(UTILSRC)/imake.includes/Imake.tmpl \
	$(UTILSRC)/imake.includes/Imake.rules \
	$(UTILSRC)/imake.includes/$(MACROFILE)
		-rm -f Makefile.bak; mv Makefile Makefile.bak
		$(IMAKE_CMD) -DTOPDIR=$(TOP)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

